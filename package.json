{
  "name": "create-gexp",
  "version": "2.0.4",
  "description": "CLI to scaffold an express project",
  "main": "scaffold.js",
  "type": "module",
  "bin": {
    "create-gexp": "dist/scaffold.js"
  },
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "keywords": [
    "cli",
    "scaffold",
    "generator",
    "gea",
    "express",
    "typescript"
  ],
  "author": "Marcos Silva",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "picocolors": "^1.1.0"
  },
  "files": [
    "dist",
    "template-express-js",
    "template-express-ts"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
